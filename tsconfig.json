{
    "compilerOptions": {
        "lib": ["ES2015", "dom"],
        "module": "commonjs",
        "target": "es5",
        "sourceMap": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "downlevelIteration": true,
        "paths": {
          "MiddlewarePackage": ["dependencies/MiddlewarePackage"],
          "ConstantsPackage": ["dependencies/ConstantsPackage"]
        }
    },
    "exclude": [
        "**/node_modules/**",
        "**/dist/**"
    ]
}
