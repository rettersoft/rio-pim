{
    "compilerOptions": {
        "lib": ["ES2015", "dom"],
        "module": "commonjs",
        "target": "es5",
        "sourceMap": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "downlevelIteration": true,
        "paths": {
          "PIMMiddlewarePackage": ["dependencies/PIMMiddlewarePackage"],
          "PIMModelsPackage": ["dependencies/PIMModelsPackage"],
          "PIMRepositoryPackage": ["dependencies/PIMRepositoryPackage"],
        }
    },
    "exclude": [
        "**/node_modules/**",
        "**/dist/**"
    ]
}
